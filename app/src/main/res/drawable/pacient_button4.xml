<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:top="4dp"
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#404040" />
            <corners android:radius="40dp" />
        </shape>
    </item>

    <item android:drawable="@drawable/ic_pacient_person"
        android:top="4dp"
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"/>

    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">

            <stroke
                android:width="1dp"
                android:color="#9C27B0"/>

        </shape>
    </item>

</layer-list>